{
  "cache": {
    "cachePriming": {
      "urls": [
        "/about-us"
      ]
    }
  },
  "redirects": [
    {
      "source": "/ssr-render",
      "destination": "/ssr",
      "statusCode": 308,
      "response": {
        "headers": {
          "x-powered-by": "launch"
        }
      }
    }
  ],
  "rewrites": [
    {
      "source": "/ssr",
      "destination": "/ssg",
      "request": {
        "headers": {
          "x-api-key": "api-key"
        }
      },
      "response": {
        "headers": {
          "x-powered-by": "launch"
        }
      }
    }
  ]
}

